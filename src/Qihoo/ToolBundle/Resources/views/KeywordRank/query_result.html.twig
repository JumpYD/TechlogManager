<h4>总数：{{ total }}</h4><br/>
<table id="logo_table" cellpadding="0" cellspacing="0" border="0" class="stdtable">
    <thead class="center">
        <tr>
            <td width="5%">id</td>
            <td width="10%">关键字</td>
            <td width="10%">应用ID列表</td>
            <td width="5%">操作人</td>
            <td width="10%">修改时间</td>
            <td width="10%">操作</td>
        </tr>
    </thead>
    <tbody class="center">
        {% for entity in entities %}
            <tr>
                <td>{{ entity.id }}</td>
                <td>{{ entity.keyword}}</td>
                <td>{{ entity.softIdList}}</td>
                <td>{{ entity.operator }}</td>
                <td>{{ entity.editTime | date('Y-m-d H:i:s') }}</td>
                
                <td class="actions">
                    <a href="{{ path('tool_keyword_modify', {"id" : entity.id}) }}" target="_self">修改</a>
                    <a href="{{ path('tool_keyword_delete', {"id" : entity.id}) }}" target="_self">删除</a>
                </td>
            </tr>
        {% endfor %}
    </tbody>
</table>
